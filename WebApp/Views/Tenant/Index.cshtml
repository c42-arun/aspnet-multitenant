@model IEnumerable<WebApp.Models.Tenant>

@{
    Layout = null;
}

<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>List of Tenants</title>
</head>
<body>
    <div>
        <ul>
            @foreach (var tenant in Model)
            {
                <li>@tenant.Name</li>
                <li>...@tenant.DomainName</li>
            }
        </ul>
    </div>
</body>
</html>